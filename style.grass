node {
  diameter: 50px;
  color: #DE9BF9;
  border-color: #BF85D6;
  border-width: 2px;
  text-color-internal: #FFFFFF;
  font-size: 10px;
}
relationship {
  color: #A5ABB6;
  shaft-width: 1px;
  font-size: 8px;
  padding: 3px;
  text-color-external: #000000;
  text-color-internal: #FFFFFF;
  caption: "<type>";
}
node.Token {
  caption: "{text}";
  color: #F16667;
  border-color: #eb2728;
  text-color-internal: #FFFFFF;
}
node.Text {
  caption: "{id}";
  color: #FFC454;
  border-color: #d7a013;
  text-color-internal: #2A2C34;
}
